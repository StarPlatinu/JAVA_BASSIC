package fa.training.main;

import fa.training.entities.Sedan;

public class MyOwnAutoShop {
     public static void main(String[] args) {
		Sedan s1 = new Sedan(100, 1000, "black", 25);
		Sedan s2 = new Sedan(100, 1000, "white", 19);
		System.out.println(s1);
		System.out.println("Discount price: "+s1.getSalePrice());
		System.out.println(s2);
		System.out.println("Discount price: "+s2.getSalePrice());
	}
}
