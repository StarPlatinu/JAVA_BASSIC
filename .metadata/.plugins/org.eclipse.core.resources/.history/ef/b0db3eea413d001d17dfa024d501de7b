package fa.training.JPE.S.A301;

public class Exercise_1 {
	 static float SumInt(float m,float n) {	 
	  if(m>n) {
		float temp = m;
		m = n;
		n = temp;
	  }
	  int sum =0;
	  while(m<=n) {
		sum+=m;
		m++;
	  }
  	   return sum;
     }

	public static void main(String[] args) {
		System.out.println("Sum of m to n: "+SumInt(1, 7));
	}

}
