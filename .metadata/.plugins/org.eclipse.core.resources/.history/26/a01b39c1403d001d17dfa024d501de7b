package fa.training.JPE.S.A301;

public class Exercise_1 {
	 static int SumInt(int m,int n) {
		 
	  if(m>n) {
		int temp = m;
		m = n;
		n = temp;
	  }
	  int sum =0;
	  while(m<=n) {
		sum+=m;
	  }
  	   return sum;
     }

	public static void main(String[] args) {
		int sumInt = SumInt(1, 10);
		System.out.println("Sum of m to n: "+sumInt);
	}

}
