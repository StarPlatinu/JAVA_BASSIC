<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
	<display-name>Car_Park_Management_System</display-name>
	<welcome-file-list>
		<welcome-file>Login.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>LoginController</servlet-name>
		<servlet-class>fa.training.controller.LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	 <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>fa.training.controller.LogoutController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>fa.training.controller.RegisterController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterController</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
      <servlet>
        <servlet-name>AddBookingController</servlet-name>
        <servlet-class>fa.training.controller.AddBookingController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddBookingController</servlet-name>
        <url-pattern>/addBooking</url-pattern>
    </servlet-mapping>
      <servlet>
        <servlet-name>BookingListController</servlet-name>
        <servlet-class>fa.training.controller.BookingListController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BookingListController</servlet-name>
        <url-pattern>/bookingList</url-pattern>
    </servlet-mapping>
</web-app>